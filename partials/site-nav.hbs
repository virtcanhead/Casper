<nav class="site-nav">
    <div class="site-nav-left">
        {{^is "home"}}
            {{#if @site.logo}}
                <a class="site-nav-logo" href="{{@site.url}}"><img src="{{@site.logo}}" alt="{{@site.title}}" /></a>
            {{else}}
                <a class="site-nav-logo" href="{{@site.url}}">{{@site.title}}</a>
            {{/if}}
        {{/is}}
        {{#if @site.navigation}}
            {{navigation}}
        {{/if}}
    </div>
    <div class="site-nav-right">
        <div class="social-links">
            <a class="social-link social-link-tw" href="https://space.bilibili.com/19523038" title="Bilibili" target="_blank" rel="noopener">{{> "icons/bilibili"}}</a>
            <a class="social-link social-link-tw" href="https://weibo.com/canhead" title="Weibo" target="_blank" rel="noopener">{{> "icons/weibo"}}</a>
            <a class="social-link social-link-tw" href="https://twitter.com/virtcanhead" title="Twitter" target="_blank" rel="noopener">{{> "icons/twitter"}}</a>
            <a class="social-link social-link-tw" href="https://github.com/virtcanhead" title="Github" target="_blank" rel="noopener">{{> "icons/github"}}</a>
        </div>
        {{#if @labs.subscribers}}
            <a class="subscribe-button" href="#subscribe">Subscribe</a>
        {{else}}
            <a class="rss-button" href="{{@site.url}}/rss/" title="RSS" target="_blank" rel="noopener">{{> "icons/rss"}}</a>
        {{/if}}
    </div>
</nav>
